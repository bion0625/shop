<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>
	
	<typeAliases>
		<typeAlias type="vo.QnaVO" alias="qna"/>
		<typeAlias type="vo.LossVO" alias="loss"/>
		<typeAlias type="vo.NoticeVO" alias="notice"/>
		<typeAlias type="vo.FreeVO" alias="free"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="config/mybatis/mapper/bus.xml" />
		<mapper resource="config/mybatis/mapper/qna.xml" />
		<mapper resource="config/mybatis/mapper/loss.xml" />
		<mapper resource="config/mybatis/mapper/notice.xml" />
		<mapper resource="config/mybatis/mapper/free.xml" />
	</mappers>
</configuration>














